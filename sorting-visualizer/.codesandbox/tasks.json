    "dev": {
      "name": "dev",
      "command": "npm run dev",
      "runAtStart": true
    },
    "build": {
      "name": "build",
      "command": "npm run build",
      "runAtStart": false
    },
    "lint": {
      "name": "lint",
      "command": "npm run lint",
      "runAtStart": false
    },
    "preview": {
      "name": "preview",
      "command": "npm run preview",
      "runAtStart": false
    },
    "test": {
      "name": "test",
      "command": "npm run test",
      "runAtStart": false
    },
    "cypress:open": {
      "name": "cypress:open",
      "command": "npm run cypress:open",
      "runAtStart": false
    },
    "cypress:run": {
      "name": "cypress:run",
      "command": "npm run cypress:run",
      "runAtStart": false
    },
    "e2e:open": {
      "name": "e2e:open",
      "command": "npm run e2e:open",
      "runAtStart": false
    },
    "e2e:test": {
      "name": "e2e:test",
